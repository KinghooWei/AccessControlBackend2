<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.cms.mapper.PassengerMapper">

    <select id="getPassengerByFlight" resultType="com.example.cms.bean.PassengerBean">
        SELECT * FROM passenger WHERE flight = #{flight} and date = #{date}
    </select>

</mapper>